DECLSPEC u32x hc_bytealign_be (const u32x a, const u32x b, const int c)
{
  u32x r = 0;

  switch (c & 3)
  {
    case 0: r =              b;        break;
    case 1: r = (a << 24) | (b >>  8); break;
    case 2: r = (a << 16) | (b >> 16); break;
    case 3: r = (a <<  8) | (b >> 24); break;
  }

  return r;
}

