DECLSPEC u32x hc_bytealign (const u32x a, const u32x b, const int c)
{
  const int c_mod_4 = c & 3;

  const int c_minus_4 = 4 - c_mod_4;

  const u32x r = hc_byte_perm (a, b, (0x76543210 >> (c_minus_4 * 4)) & 0xffff);

  return r;
}

